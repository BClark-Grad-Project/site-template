include ../mixins/navigation.jade

if userType(user.authentications) === 'guest'
  include login.jade
  include register.jade
  include app.jade

nav.navbar.navbar-inverse(role='navigation',style='margin:0px;padding:0px;')
  div(style='position:absolute;top:0;bottom:0;left:0;right:0;background-image:url("/images/survey/digital-bg.png");background-attachment:fixed;background-size:cover;opacity:0.3;')
  .container-fluid(style='margin:0px;padding-left:0px;')
    .navbar-header.navbar-left.col-xs-5.col-sm-2.col-md-2.col-lg-1(style='padding:0px; margin:0px;')
      a.collapsed.navbar-toggle(data-toggle='collapse', data-target='#navigation', style='position:relative;float:left;border:0px;padding:0px;margin:0px;')
        small(style='position:absolute;float:right;bottom:0;top:60;left:200;right:0;color:white;opacity:0.4; font-size:10px;') Developer
        h4(style='margin:0px;padding:10px;white-space:nowrap;')
          img(src='/images/site/logo-ico.png', style='margin-top:0px;padding-top:0px;height:30px;')
          |  Mind Research   
          span.caret.text-muted(style='margin:0px;padding:0px;margin-right:0px;')
      .navbar-brand.hide-sm(style='position:relative;float:left;padding:0px;border:0px;margin:0px;margin-top:0px;padding-top:0px;')
        small(style='position:absolute;float:right;top:60;bottom:0;left:220;right:0;color:white;opacity:0.4; font-size:10px;') Developer
        a(href='/', style='margin:0px;padding:0px;margin-top:0px;padding-top:0px;float:left;')        
          h4(style='margin:0px;padding:10px;white-space:nowrap;')
            img(src='/images/site/logo-ico.png', style='margin-top:0px;padding-top:0px;height:30px;')
            |  Mind Research  
    .col-xs-0.col-sm-1.col-md-1.col-lg-1(style='padding:0px; margin:0px;')
    #navigation.collapse.navbar-collapse.navbar-left.col-xs-12.col-sm-4.col-md-4.col-lg-6(style='padding:0px; margin:0px;')
      +navLinks()
      if userType(user.authorizations) !== 'guest'
        +appLinks()
    #usernavigation.navbar-right.col-xs-6.col-sm-4.col-md-5.col-lg-4(style='padding:0px; margin:0px;float:right;')  
      +navUser()