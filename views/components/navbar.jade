include ../mixins/navigation.jade

nav.navbar.navbar-inverse(role='navigation',style='margin:0px;padding:0px;')
  .container-fluid(style='margin:0px;padding-left:0px;')
    .navbar-header.navbar-left.col-xs-8.col-sm-3.col-md-3.col-lg-2(style='padding:0px; margin:0px;')
      a.collapsed.navbar-toggle(data-toggle='collapse', data-target='#navigation', style='float:left;border:0px;padding:0px;padding-top:10px;margin:0px;')
        img(src='/images/site/logo6.png', style='padding:6px 20px; margin:0px;')
        span.caret.text-muted(style='margin:0px;padding:0px;margin-right:10px;')
      .navbar-brand.hide-sm(style='float:left;padding:0px;border:0px;padding-top:10px;margin:0px;')
        a(href='/')
          img(src='/images/site/logo6.png', style='padding:0px 10px; margin:0px;')  
    #navigation.collapse.navbar-collapse.navbar-left.col-xs-12.col-sm-4.col-md-3.col-lg-5(style='padding:0px; margin:0px;')
      +navLinks()
    #usernavigation.navbar-right.col-xs-4.col-sm-5.col-md-6.col-lg-5(style='padding:0px; margin:0px;float:right;')  
      +navUser()
                      
if user.credentials.type == 'guest'
  include login.jade
  include register.jade